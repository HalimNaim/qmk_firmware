AUTO_SHIFT_ENABLE = yes
MOUSEKEY_ENABLE = no

BOOTMAGIC_ENABLE = no      # Enable Bootmagic Lite
EXTRAKEY_ENABLE = no       # Audio control and System control
CONSOLE_ENABLE = no        # Console for debug
COMMAND_ENABLE = no        # Commands for debug and configuration
NKRO_ENABLE = no           # Enable N-Key Rollover
BACKLIGHT_ENABLE = no      # Enable keyboard backlight functionality
AUDIO_ENABLE = no          # Audio output

OLED_ENABLE = no           # Enables the use of OLED displays
ENCODER_ENABLE = no        # Enables the use of one or more encoders
RGBLIGHT_ENABLE = no       # Enable keyboard RGB underglow
RGB_MATRIX_ENABLE = no     # Enable keyboard RGB matrix (do not use together with RGBLIGHT_ENABLE)
TAP_DANCE_ENABLE = no      # The documentation claims that this adds almost 1k

CAPS_WORD_ENABLE = yes
ALLOW_WARNINGS = no        # Needed for older tags. `[-Warray-bounds]' turns into an error if not set
